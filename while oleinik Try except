while True:
    try:
        a=int(input("Sisesta kolmnurga a külje paikus:"))
        break
    except:
        print("Viga palun sisesta täisarv!")


while True:
    try:
        b=int(input("Sisesta kolmnurga b külje paikus:"))
        break
    except:
        print("Viga palun sisesta täisarv!")



while True:
    try:
        c=int(input("Sisesta kolmnurga c külje paikus:"))
        break
    except:
        print("Viga palun sisesta täisarv!")

if a>0 and b>0 and c>0:
    print("kõik kuljed on positiivsed")
    print("Kontrollime kas nad on ühe kolmnurga küljed")
    if a+b>c and a+c>b and b+c>a:
        print("Jah need on ühe kolmnurga küljed")
        p=a+b+c 
        print(f"Kolmnurga ümbermõõt on:{p}")
    else:
        print("Ei,sellist kolmnurka ei saa olemas olla")

print("Lõpp")



