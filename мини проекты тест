    if ch in freq:
        freq[ch] += 1
    else:
        freq[ch] = 1

for key in freq:
    print(key, ":", freq[key])

4Ô∏è‚É£ –¢–µ–ª–µ—Ñ–æ–Ω–Ω–∞—è –∫–Ω–∏–≥–∞ (—Å–ª–æ–≤–∞—Ä—å + –º–µ–Ω—é)
phonebook = {}

while True:
    print("1 - –î–æ–±–∞–≤–∏—Ç—å")
    print("2 - –ù–∞–π—Ç–∏")
    print("3 - –£–¥–∞–ª–∏—Ç—å")
    print("4 - –í—ã—Ö–æ–¥")

    choice = input("–í—ã–±–æ—Ä: ")

    if choice == "1":
        name = input("–ò–º—è: ")
        phone = input("–¢–µ–ª–µ—Ñ–æ–Ω: ")
        phonebook[name] = phone

    elif choice == "2":
        name = input("–ò–º—è: ")
        if name in phonebook:
            print(phonebook[name])
        else:
            print("–ù–µ—Ç —Ç–∞–∫–æ–≥–æ –∫–æ–Ω—Ç–∞–∫—Ç–∞")

    elif choice == "3":
        name = input("–ò–º—è: ")
        if name in phonebook:
            del phonebook[name]
            print("–£–¥–∞–ª–µ–Ω–æ")
        else:
            print("–ù–µ—Ç —Ç–∞–∫–æ–≥–æ –∫–æ–Ω—Ç–∞–∫—Ç–∞")

    elif choice == "4":
        break

