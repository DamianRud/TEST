 —à–∏—Ñ—Ä–∞—Ç–æ—Ä
# –ê–ª—Ñ–∞–≤–∏—Ç
alphabet = list("abcdefghijklmnopqrstuvwxyz")

# –§—É–Ω–∫—Ü–∏—è —à–∏—Ñ—Ä–æ–≤–∞–Ω–∏—è
def encrypt(text, shift):
    result = ""
    for char in text.lower():
        try:
            index = alphabet.index(char)          # –ü–æ–ø—ã—Ç–∫–∞ –Ω–∞–π—Ç–∏ –±—É–∫–≤—É –≤ —Å–ø–∏—Å–∫–µ
            new_index = (index + shift) % 26     # –°–¥–≤–∏–≥ –ø–æ –º–æ–¥—É–ª—é 26
            result += alphabet[new_index]
        except ValueError:
            # –ï—Å–ª–∏ —Å–∏–º–≤–æ–ª–∞ –Ω–µ—Ç –≤ —Å–ø–∏—Å–∫–µ (–Ω–∞–ø—Ä–∏–º–µ—Ä, –ø—Ä–æ–±–µ–ª –∏–ª–∏ –∑–Ω–∞–∫), –æ—Å—Ç–∞–≤–ª—è–µ–º –∫–∞–∫ –µ—Å—Ç—å
            result += char
    return result

# –§—É–Ω–∫—Ü–∏—è —Ä–∞—Å—à–∏—Ñ—Ä–æ–≤–∫–∏
def decrypt(text, shift):
    return encrypt(text, -shift)

# –û—Å–Ω–æ–≤–Ω–æ–π –∫–æ–¥ —Å –æ–±—Ä–∞–±–æ—Ç–∫–æ–π –æ—à–∏–±–æ–∫ –≤–≤–æ–¥–∞
try:
    text = input("–í–≤–µ–¥–∏—Ç–µ —Ç–µ–∫—Å—Ç: ")
    shift = int(input("–í–≤–µ–¥–∏—Ç–µ —Å–¥–≤–∏–≥ (—á–∏—Å–ª–æ): "))
    
    encrypted = encrypt(text, shift)
    print("–ó–∞—à–∏—Ñ—Ä–æ–≤–∞–Ω–Ω—ã–π —Ç–µ–∫—Å—Ç:", encrypted)
    
    decrypted = decrypt(encrypted, shift)
    print("–†–∞—Å—à–∏—Ñ—Ä–æ–≤–∞–Ω–Ω—ã–π —Ç–µ–∫—Å—Ç:", decrypted)

except ValueError:
    print("–û—à–∏–±–∫–∞! –°–¥–≤–∏–≥ –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å —Ü–µ–ª—ã–º —á–∏—Å–ª–æ–º.")






–ø–æ–∑–∏—Ç–∏–≤–Ω—ã–µ –æ—Ç—Ä–µ—Ü–∞—Ç–µ–ª—å–Ω—ã–µ –∏ –Ω—É–ª–∏

try:
    n = int(input("–í–≤–µ–¥–∏—Ç–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —á–∏—Å–µ–ª: "))

    positive = 0
    negative = 0
    zeros = 0

    for i in range(n):
        try:
            number = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: "))

            if number > 0:
                positive += 1
            elif number < 0:
                negative += 1
            else:
                zeros += 1

        except ValueError:
            print("–û—à–∏–±–∫–∞! –ù—É–∂–Ω–æ –≤–≤–µ—Å—Ç–∏ —Ü–µ–ª–æ–µ —á–∏—Å–ª–æ.")
            i -= 1

    print("–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã—Ö —á–∏—Å–µ–ª:", positive)
    print("–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –æ—Ç—Ä–∏—Ü–∞—Ç–µ–ª—å–Ω—ã—Ö —á–∏—Å–µ–ª:", negative)
    print("–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –Ω—É–ª–µ–π:", zeros)

except ValueError:
    print("–û—à–∏–±–∫–∞! –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —á–∏—Å–µ–ª –¥–æ–ª–∂–Ω–æ –±—ã—Ç—å —Ü–µ–ª—ã–º.")















