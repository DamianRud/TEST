from prettytable import PrettyTable
from prettytable import PrettyTable

info = (
    ("34", "–ò–≤–∞–Ω", "–ú–æ—Å–∫–≤–∞"),
    ("23", "–°–µ—Ä–≥–µ–π", "–°–∞–Ω–∫—Ç-–ü–µ—Ç–µ—Ä–±—É—Ä–≥"),
    ("30", "–ê–ª–µ–∫—Å–µ–π–ê–ª–µ–∫—Å–µ–π–ê–ª–µ–∫—Å–µ–π–ê–ª–µ–∫—Å–µ–π", "–ú–æ—Å–∫–≤–∞"),
    ("20", "–ê–Ω–¥—Ä–µ–π", "–ö–∏—Ä–æ–≤"),
)

table = PrettyTable(("–í–æ–∑—Ä–∞—Å—Ç", "–ò–º—è", "–ì–æ—Ä–æ–¥"))

for person in info:
    table.add_row(person)

print(table)










