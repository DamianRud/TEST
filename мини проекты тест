print("–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å–ª–æ–≤:", len(words))
print("–°–∞–º–æ–µ –¥–ª–∏–Ω–Ω–æ–µ —Å–ª–æ–≤–æ:", longest)

9Ô∏è‚É£ –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–∞—Ä–æ–ª—è (–±–µ–∑ —Ä–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏–∏)
password = input("–í–≤–µ–¥–∏—Ç–µ –ø–∞—Ä–æ–ª—å: ")

has_digit = False
has_lower = False
has_upper = False
has_special = False

specials = "!@#$%^&*()_+-="

for ch in password:
    if ch.isdigit():
        has_digit = True
    elif ch.islower():
        has_lower = True
    elif ch.isupper():
        has_upper = True
    elif ch in specials:
        has_special = True

if has_digit and has_lower and has_upper and has_special:
    print("–ü–∞—Ä–æ–ª—å –Ω–∞–¥—ë–∂–Ω—ã–π")
else:
    print("–ü–∞—Ä–æ–ª—å —Å–ª–∞–±—ã–π")



