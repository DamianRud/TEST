–£–≥–∞–¥–∞–π —á–∏—Å–ª–æ (—Å –ø—Ä–æ–≤–µ—Ä–∫–æ–π)
import random

number = random.randint(1, 100)
attempts = 0

while True:
    try:
        guess = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: "))
        attempts += 1

        if guess < number:
            print("–ë–æ–ª—å—à–µ")
        elif guess > number:
            print("–ú–µ–Ω—å—à–µ")
        else:
            print("–£–≥–∞–¥–∞–ª –∑–∞", attempts, "–ø–æ–ø—ã—Ç–æ–∫")
            break
    except:
        print("–í–≤–µ–¥–∏—Ç–µ —Ü–µ–ª–æ–µ —á–∏—Å–ª–æ")




4Ô∏è‚É£ –ö–æ–Ω–≤–µ—Ä—Ç–µ—Ä –µ–¥–∏–Ω–∏—Ü

def c_to_f(c):
    return c * 9 / 5 + 32

try:
    c = float(input("–í–≤–µ–¥–∏—Ç–µ —Ç–µ–º–ø–µ—Ä–∞—Ç—É—Ä—É: "))
    print("–§–∞—Ä–µ–Ω–≥–µ–π—Ç:", c_to_f(c))
except:
    print("–û—à–∏–±–∫–∞ –≤–≤–æ–¥–∞")











5Ô∏è‚É£ –†–∞–±–æ—Ç–∞ —Å–æ —Å–ø–∏—Å–∫–æ–º —á–∏—Å–µ–ª
numbers = []

try:
    n = int(input("–°–∫–æ–ª—å–∫–æ —á–∏—Å–µ–ª: "))
    for i in range(n):
        numbers.append(int(input("–ß–∏—Å–ª–æ: ")))

    print("–°—É–º–º–∞:", sum(numbers))
    print("–ú–∞–∫—Å–∏–º—É–º:", max(numbers))

except:
    print("–û—à–∏–±–∫–∞ –≤–≤–æ–¥–∞")







6Ô∏è‚É£ –§—É–Ω–∫—Ü–∏—è + –ø—Ä–æ–≤–µ—Ä–∫–∞ (—á—ë—Ç–Ω–æ—Å—Ç—å)
def is_even(n):
    return n % 2 == 0

try:
    x = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: "))
    if is_even(x):
        print("–ß—ë—Ç–Ω–æ–µ")
    else:
        print("–ù–µ—á—ë—Ç–Ω–æ–µ")
except:
    print("–û—à–∏–±–∫–∞")


7Ô∏è‚É£ –ë–ª–æ–∫-—Å—Ö–µ–º–Ω–∞—è –∑–∞–¥–∞—á–∞ (—á—ë—Ç–Ω—ã–µ —á–∏—Å–ª–∞)
try:
    n = int(input("–í–≤–µ–¥–∏—Ç–µ N: "))
    for i in range(1, n + 1):
        if i % 2 == 0:
            print(i)
except:
    print("–û—à–∏–±–∫–∞ –≤–≤–æ–¥–∞")








–º–Ω–æ–≥–æ —Ñ—É–Ω–∫—Ü–∏–æ–Ω–∞–ª—å–Ω—ã–π  TO-DO list 
tasks = []

# –∑–∞–≥—Ä—É–∑–∫–∞ –∏–∑ —Ñ–∞–π–ª–∞
try:
    file = open("todo.txt", "r")
    for line in file:
        tasks.append(line.strip())
    file.close()
except FileNotFoundError:
    pass


while True:
    print("\n–ú–ï–ù–Æ:")
    print("1 - –î–æ–±–∞–≤–∏—Ç—å –∑–∞–¥–∞—á—É (append)")
    print("2 - –í—Å—Ç–∞–≤–∏—Ç—å –∑–∞–¥–∞—á—É (insert)")
    print("3 - –£–¥–∞–ª–∏—Ç—å –ø–æ –Ω–∞–∑–≤–∞–Ω–∏—é (remove)")
    print("4 - –£–¥–∞–ª–∏—Ç—å –ø–æ –Ω–æ–º–µ—Ä—É (pop)")
    print("5 - –ù–∞–π—Ç–∏ –∏–Ω–¥–µ–∫—Å –∑–∞–¥–∞—á–∏ (index)")
    print("6 - –ü–æ–¥—Å—á–∏—Ç–∞—Ç—å –æ–¥–∏–Ω–∞–∫–æ–≤—ã–µ –∑–∞–¥–∞—á–∏ (count)")
    print("7 - –û—Ç—Å–æ—Ä—Ç–∏—Ä–æ–≤–∞—Ç—å (sort)")
    print("8 - –†–∞–∑–≤–µ—Ä–Ω—É—Ç—å —Å–ø–∏—Å–æ–∫ (reverse)")
    print("9 - –û—á–∏—Å—Ç–∏—Ç—å —Å–ø–∏—Å–æ–∫ (clear)")
    print("10 - –ü–æ–∫–∞–∑–∞—Ç—å —Å–ø–∏—Å–æ–∫")
    print("0 - –í—ã—Ö–æ–¥")

    choice = input("–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ: ")

    # append
    if choice == "1":
        task = input("–í–≤–µ–¥–∏—Ç–µ –∑–∞–¥–∞—á—É: ")
        tasks.append(task)

    # insert
    elif choice == "2":
        try:
            task = input("–í–≤–µ–¥–∏—Ç–µ –∑–∞–¥–∞—á—É: ")
            pos = int(input("–í–≤–µ–¥–∏—Ç–µ –ø–æ–∑–∏—Ü–∏—é: "))
            tasks.insert(pos - 1, task)
        except:
            print("–û—à–∏–±–∫–∞ –≤—Å—Ç–∞–≤–∫–∏")

    # remove
    elif choice == "3":
        task = input("–í–≤–µ–¥–∏—Ç–µ –∑–∞–¥–∞—á—É –¥–ª—è —É–¥–∞–ª–µ–Ω–∏—è: ")
        try:
            tasks.remove(task)
        except:
            print("–¢–∞–∫–æ–π –∑–∞–¥–∞—á–∏ –Ω–µ—Ç")

    # pop
    elif choice == "4":
        try:
            num = int(input("–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä –∑–∞–¥–∞—á–∏: "))
            removed = tasks.pop(num - 1)
            print("–£–¥–∞–ª–µ–Ω–æ:", removed)
        except:
            print("–û—à–∏–±–∫–∞ —É–¥–∞–ª–µ–Ω–∏—è")

    # index
    elif choice == "5":
        task = input("–í–≤–µ–¥–∏—Ç–µ –∑–∞–¥–∞—á—É: ")
        try:
            print("–ò–Ω–¥–µ–∫—Å:", tasks.index(task))
        except:
            print("–ó–∞–¥–∞—á–∞ –Ω–µ –Ω–∞–π–¥–µ–Ω–∞")

    # count
    elif choice == "6":
        task = input("–í–≤–µ–¥–∏—Ç–µ –∑–∞–¥–∞—á—É: ")
        print("–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ:", tasks.count(task))

    # sort
    elif choice == "7":
        tasks.sort()
        print("–°–ø–∏—Å–æ–∫ –æ—Ç—Å–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω")

    # reverse
    elif choice == "8":
        tasks.reverse()
        print("–°–ø–∏—Å–æ–∫ –ø–µ—Ä–µ–≤—ë—Ä–Ω—É—Ç")

    # clear
    elif choice == "9":
        tasks.clear()
        print("–°–ø–∏—Å–æ–∫ –æ—á–∏—â–µ–Ω")

    # show
    elif choice == "10":
        if len(tasks) == 0:
            print("–°–ø–∏—Å–æ–∫ –ø—É—Å—Ç")
        else:
            for i in range(len(tasks)):
                print(i + 1, tasks[i])

    # exit
    elif choice == "0":
        break

    else:
        print("–ù–µ–≤–µ—Ä–Ω—ã–π –≤—ã–±–æ—Ä")


# —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –≤ —Ñ–∞–π–ª
file = open("todo.txt", "w")
for task in tasks:
    file.write(task + "\n")
file.close()

















(1) –ü—Ä–æ—Å—Ç—ã–µ –∞—Ä–∏—Ñ–º–µ—Ç–∏—á–µ—Å–∫–∏–µ –æ–ø–µ—Ä–∞—Ü–∏–∏
def arithmetic(a, b, op):
    if op == "+":
        return a + b
    elif op == "-":
        return a - b
    elif op == "*":
        return a * b
    elif op == "/":
        if b == 0:
            return "–î–µ–ª–µ–Ω–∏–µ –Ω–∞ –Ω–æ–ª—å"
        return a / b
    else:
        return "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –æ–ø–µ—Ä–∞—Ü–∏—è"

(2) –í–∏—Å–æ–∫–æ—Å–Ω—ã–π –≥–æ–¥
def is_year_leap(year):
    if year % 400 == 0:
        return True
    if year % 100 == 0:
        return False
    if year % 4 == 0:
        return True
    return False
user=int(input("Sisesta aasta:"))
print(is_year_leap(user))

(3) –ö–≤–∞–¥—Ä–∞—Ç (–ø–µ—Ä–∏–º–µ—Ç—Ä, –ø–ª–æ—â–∞–¥—å, –¥–∏–∞–≥–æ–Ω–∞–ª—å)
import math

def square(a):
    perimeter = 4 * a
    area = a * a
    diagonal = a * math.sqrt(2)
    return perimeter, area, diagonal

(4) –í—Ä–µ–º–µ–Ω–∞ –≥–æ–¥–∞
def season(month):
    if month == 12 or month == 1 or month == 2:
        return "–∑–∏–º–∞"
    elif month >= 3 and month <= 5:
        return "–≤–µ—Å–Ω–∞"
    elif month >= 6 and month <= 8:
        return "–ª–µ—Ç–æ"
    elif month >= 9 and month <= 11:
        return "–æ—Å–µ–Ω—å"
    else:
        return "–ù–µ–≤–µ—Ä–Ω—ã–π –º–µ—Å—è—Ü"

(5) –ë–∞–Ω–∫–æ–≤—Å–∫–∏–π –¥–µ–ø–æ–∑–∏—Ç
def bank(a, years):
    for i in range(years):
        a = a + a * 0.1
    return a

(6) –ü—Ä–æ—Å—Ç–æ–µ —á–∏—Å–ª–æ + –¥–µ–ª–∏—Ç–µ–ª–∏
def is_prime(n):
    if n < 2:
        return False

    divisors = []
    for i in range(1, n + 1):
        if n % i == 0:
            divisors.append(i)

    if len(divisors) == 2:
        return True
    else:
        print("–î–µ–ª–∏—Ç–µ–ª–∏:", divisors)
        return False

(7) –ü—Ä–∞–≤–∏–ª—å–Ω–∞—è –¥–∞—Ç–∞
def date(day, month, year):
    if month < 1 or month > 12:
        return False

    days_in_month = [31, 28, 31, 30, 31, 30,
                     31, 31, 30, 31, 30, 31]

    if is_year_leap(year):
        days_in_month[1] = 29

    if day < 1 or day > days_in_month[month - 1]:
        return False

    return True

day = int(input("–í–≤–µ–¥–∏—Ç–µ –¥–µ–Ω—å: "))
month = int(input("–í–≤–µ–¥–∏—Ç–µ –º–µ—Å—è—Ü: "))
year = int(input("–í–≤–µ–¥–∏—Ç–µ –≥–æ–¥: "))

